    {% include "vendor-criteo" %}
    {% include "vendor-facebook-og" %}
    {% include "vendor-twitter-cards" %}
    {% include "vendor-klaviyo" %}
    {% include "vendor-swell" %}
    {% include "vendor-yotpo" %}
    {% include "vendor-pinterest" %}
    {% include "vendor-reddit" %}

    {% include 'css-liquid-vars' %}
    {% include 'css-swatch-colors' %}
    {{ 'theme.min.css' | asset_url | stylesheet_tag }}

    {% comment %} Sadly cannot find an NPM module for this: {% endcomment %}
    {%- if template contains "customers" -%}
      {{ "shopify_common.js"  | shopify_asset_url | script_tag }}
      {{ "customer_area.js"  | shopify_asset_url | script_tag }}
