.my-rewards-link {
  color: #337ab7;
}
.sr_wrapper_main {
  font-size: 14px;
  color: $default_clr;
  &,
  * {
    box-sizing: border-box;
    line-height: 1.2;
    margin: 0;
    padding: 0;
    @include font($pr,
    normal);
    list-style: none;
  }
  a {
    text-decoration: none;
  }
  .desktop {
    display: none;
    @media(min-width: 768px) {
      display: block;
    }
  }
  .mobile {
    display: block;
    @media(min-width: 768px) {
      display: none;
    }
  }
  .clr_scnd {
    color: $scnd;
  }
  .sec_space {
    height: 80px;
    @media (max-width: 767px) {
      height: 50px;
    }
  }
  .sec_title {
    @extend %sec_title;
  }
  .text-center {
    text-align: center;
  }
  .text-uppercase {
    text-transform: uppercase;
  }
  .container {
    max-width: 1920px;
    width: 100%;
    margin: 0 auto;
    padding: 0 15px;
  }
  // banner
  .banner {
    min-height: 450px;
    color: $white;
    position: relative;
    .banner_img {
      object-fit: cover;
      width: 100%;
      vertical-align: top;
      @media(min-width: 767px) {
        min-height: 450px;
      }
    }
    .login_user_name {
      font-size: 26px;
      margin-bottom: 10px;
    }
    .swell-checkout-dollar-balance {
      font-size: 15px;
    }
    .banner_txt_wrap {
      margin-bottom: 20px;
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      width: 100%;
      margin: 0 auto;
      top: 50%;
      -webkit-transform: translateY(-50%);
      -moz-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
      -o-transform: translateY(-50%);
      transform: translateY(-50%);
      padding-bottom: 20px;
    }
    .banner_title {
      font-size: 54px;
      @include font($grb,
      normal);
      margin: 0 0 10px;
    }
    .banner_txt {
      font-size: 13px;
    }
    .banner_btns_wrap {
      @include display(flex,
      center,
      center);
    }
    .banner_btn {
      width: 184px;
      height: 54px;
      background: $scnd;
      font-size: 13px;
      display: block;
      color: $white;
      text-decoration: none;
      @include display(flex,
      center,
      center);
      border-radius: 40px;
      margin: 20px 10px;
      border: 2px solid $scnd;
      &:hover {
        color: #ffffff;
        background: #2d2d2d;
        border-color: #2d2d2d;
      }
    }
    .status_link {
      a {
        color: $scnd;
        text-decoration: none;
      }
    }
    @media (max-width: 767px) {
      .banner_txt_wrap {
        position: relative;
        top: 0%;
        -webkit-transform: translateY(0);
        -moz-transform: translateY(0);
        -ms-transform: translateY(0);
        -o-transform: translateY(0);
        transform: translateY(0);
        background-color: #eae9e7;
        padding: 60px 15px;
        color: $black
      }
      .banner_btns_wrap {
        display: block;
      }
      .banner_btn {
        margin: 20px auto;
      }
      .banner_title {
        font-size: 30px;
      }
      .banner_txt {
        max-width: 290px;
        margin: 0 auto;
      }
      .status_link {
        a {
          display: block;
        }
      }
    }
  }
  // banner end
  // earn reward
  .er_list_wrapper {
    overflow: hidden;
    max-width: 1920px;
    width: 100%;
    margin: 0 auto;
  }
  .er_list {
    font-size: 0;
    margin-top: -1px;
    margin-left: -1px;
    &.logged-in {
      .swell-campaign {
        cursor: pointer;
      }
    }
    &>li {
      width: 20%;
      display: inline-block;
      vertical-align: top;
      border-top: 1px solid #eeeeee;
      border-left: 1px solid #eeeeee;
      position: relative;
      .swell-campaign-content {
        min-height: 240px;
        @include display(flex,
        center);
        padding: 15px 0 15px 70px;
        @media (max-width: 1400px) {
          padding: 15px 0 15px 20px;
        }
        @media (max-width: 991px) {
          padding: 15px;
        }
      }
      /*/*/
      /*/ @include display(inline-flex,'',center);*/
      display: -webkit-inline-flex;
      display: -moz-inline-flex;
      display: -ms-inline-flex;
      display: -o-inline-flex;
      display: inline-flex;
      -moz-justify-content: space-between;
      justify-content: space-between;
      -webkit-align-items: center;
      align-items: center;
      .swell-campaign-content {
        -webkit-flex-direction: column;
        -moz-flex-direction: column;
        -ms-flex-direction: column;
        -o-flex-direction: column;
        flex-direction: column;
        /*position: relative;*/
        width: 100%;
        .swell-campaign-hover {
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          width: 100%;
          height: 100%;
          display: flex;
          background: rgba(247, 247, 247, 0.98);
          align-items: center;
          justify-content: center;
          .swell-campaign-hover-content {
            .swell-link-list {
              .swell-link {
                display: block;
                a {
                  font-size: 13px;
                  font-weight: normal;
                  color: #fff;
                  border-color: #fff;
                  height: 36px;
                  display: block;
                  border: 1px solid;
                  width: 160px;
                  text-align: center;
                  border-radius: 30px;
                  padding: 3px;
                  margin: 10px 0;
                  @include display(flex,
                  center,
                  center);
                  text-transform: uppercase;
                  &.swell-link-login {
                    background: $scnd;
                    border-color: $scnd;
                  }
                  &.swell-link-register {
                    background: #fff;
                    border-color: $scnd;
                    color: $scnd;
                  }
                }
              }
            }
          }
        }
      }
      .swell-campaign-icon-content {
        font-size: 40px;
        color: #d1d1d1;
        line-height: 1;
        i {
          line-height: 1;
        }
      }
      .swell-campaign-type-content {
        font-size: 15px;
        color: #444444;
        margin: 20px 0 10px;
        display: block;
        @media (min-width: 1200px) {
          font-size: 18px;
        }
      }
      .swell-campaign-value {
        font-size: 15px;
        color: $scnd;
      }
    }
  }
  @media (min-width: 768px) and (max-width: 991px) {
    .er_list {
      &>li {
        width: 25%;
        &:nth-child(9),
        &:nth-child(10) {
          width: 50%;
        }
      }
    }
  }
  @media (max-width: 767px) {
    .earn_rwrd {
      padding: 0 15px;
    }
    .er_list {
      margin-top: 0;
      border-bottom: 1px solid #d1d1d1;
      &>li {
        width: 50%;
        border-top: 1px solid #d1d1d1;
        border-left: 1px solid #d1d1d1;
        text-align: center;
        .swell-campaign-content {
          -webkit-flex-grow: 1;
          flex-grow: 1;
        }
      }
    }
  }
  @media (max-width: 480px) {
    .er_list {
      &>li {
        width: 100%;
        .swell-campaign-content {
          min-height: 180px;
        }
      }
    }
  }
  // earn reward end
  // upgrade membership
  .eh_wrap {
    margin-left: -10px;
    margin-right: -10px;
    .eh_inner {
      min-height: 600px;
      padding: 0 10px;
      position: relative;
      display: inline-flex;
      margin: 0 0 20px;
      width: 50%;
      .mobile {
        display: none;
      }
      .desktop {
        display: block;
      }
      .eh_img {
        width: 100%;
        object-fit: cover;
        object-position: top center;
        border-radius: 3px;
      }
      .eh_img_txt {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        padding: 50px 40px;
        z-index: 2;
        color: #FFF;
        i {
          font-size: 30px;
          margin-top: 15px;
          display: inline-block;
        }
      }
      .eh_txt {
        background-color: #f7f7f7;
        position: relative;
        flex-grow: 1;
        font-size: 15px;
        border-radius: 3px;
        @include display(flex,
        center,
        center);
        .eh_txt_inner {
          padding: 70px 15px;
          width: 100%;
          @include font($bt,
          normal);
        }
      }
      .member_pkg_wrap {
        max-width: 270px;
        width: 100%;
        margin-top: 70px;
        margin-left: 5px;
        margin-right: 5px;
        h4 {
          font-size: 24px;
          @include font($grb,
          normal);
          color: #444444;
        }
        h6 {
          @include font($bt,
          normal);
          font-size: 16px;
          margin: 20px 0 45px;
          color: #969696;
        }
      }
      .member_pkg_list {
        margin-left: -5px;
        margin-right: -5px;
        @include display(flex,
        space-between);
        &.logged-in {
          .member_pkg_wrap {
            position: relative;
            padding-bottom: 50px;
            .status-holder {
              color: $scnd;
              position: absolute;
              bottom: 0;
              left: 0;
            }
          }
        }
        ul {
          li {
            margin: 0 0 20px;
            padding: 0 0 20px;
            border-bottom: 1px solid #dddddd;
            &:last-child {
              margin-bottom: 0;
            }
          }
        }
      }
    }
    &.upgrade_wrap {
      .eh_txt {
        .eh_txt_inner {
          max-width: 830px;
          margin: 0 auto;
          padding: 70px 75px;
        }
      }
    }
    &.ref_frnd_wrap {
      .eh_txt {
        .eh_txt_inner {
          max-width: 360px;
          margin: 0 auto;
          i {
            font-size: 35px;
            margin-bottom: 20px;
            display: inline-block;
          }
          h3 {
            margin-bottom: 15px;
          }
          .strt_ref {
            color: #f25b29;
            border-bottom: 1px solid #f25b29;
            text-decoration: none;
            margin-top: 25px;
            display: inline-block;
          }
        }
      }
    }
    @media (max-width: 1199px) {
      .eh_inner {
        .eh_txt {
          .eh_txt_inner {
            padding: 70px 15px;
          }
        }
      }
    }
    @media (max-width: 991px) {
      &.ref_frnd_wrap,
      &.upgrade_wrap {
        display: block;
        .eh_inner {
          width: 100%;
          display: block;
          min-height: auto;
          .mobile {
            display: block;
          }
          .desktop {
            display: none;
          }
        }
      }
    }
    @media (min-width: 768px) {
      @include display(flex,
      space-between);
      &.ref_frnd_wrap {
        .eh_inner:nth-child(1) {
          -webkit-order: 2;
          -moz-order: 2;
          -ms-order: 2;
          -o-order: 2;
          order: 2;
        }
      }
    }
    @media (max-width: 767px) {
      &.upgrade_wrap {
        .sec_title {
          max-width: 190px;
          margin: 0 auto 15px;
          text-align: center;
          &+p {
            text-align: center;
            max-width: 340px;
            margin: 0 auto;
          }
        }
      }
      &.ref_frnd_wrap {
        .eh_txt {
          .eh_txt_inner {
            max-width: 270px;
          }
        }
      }
      .eh_inner {
        width: 100%;
        .member_pkg_wrap {
          margin-top: 40px;
          h6 {
            margin-bottom: 35px;
          }
        }
        .eh_img_txt {
          top: 50%;
          -webkit-transform: translateY(-50%);
          -moz-transform: translateY(-50%);
          -ms-transform: translateY(-50%);
          -o-transform: translateY(-50%);
          transform: translateY(-50%);
          text-align: center;
        }
      }
    }
    @media (max-width: 480px) {
      .eh_inner {
        .member_pkg_list {
          h4 {
            font-size: 18px;
          }
          h6 {
            font-size: 14px;
            min-height: 33px;
          }
          ul {
            li {
              margin: 0 0 10px;
              padding: 0 0 10px;
              font-size: 12px;
            }
          }
        }
      }
    }
  }
  // upgrade membership end
  // rdm_credit
  .rdm_credit {
    max-width: 385px;
    margin: 0 auto;
    h3 {
      margin-bottom: 15px;
    }
    .rewards-credit {
      margin-bottom: 10px;
    }
    .rdm_start_btn {
      @include display(flex,
      center,
      center);
      max-width: 184px;
      height: 54px;
      background: $scnd;
      border-radius: 30px;
      margin: 35px auto 15px;
      font-size: 13px;
      text-decoration: none;
      color: #fff;
      &:hover {
        color: #ffffff;
        background: #2d2d2d;
        border-color: #2d2d2d;
      }
    }
    @media (min-width: 767px) {
      .rdm_title_br {
        display: none;
      }
    }
    @media (max-width: 767px) {
      h3 {
        &+p {
          max-width: 200px;
          margin: 0 auto;
        }
      }
    }
  }
  // rdm_credit end
  // redeem product
  .redeem_products {
    max-width: 1920px;
    margin: 0 auto;
    width: 100%;
  }
  .rdm_prod_sep {
    max-width: 1130px;
    margin: 0 auto;
    width: 100;
    border-bottom: 1px solid #dadada;
  }
  .rdm_prod_slide {
    .swell-redemption-option-content:focus {
      outline: none;
    }
    .slider-arrows {
      position: absolute;
      top: 50%;
      -webkit-transform: translateY(-50%);
      -moz-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
      -o-transform: translateY(-50%);
      transform: translateY(-50%);
      width: 80px;
      height: 80px;
      border-radius: 50%;
      background: #fff;
      @include display(flex,
      center,
      center);
      z-index: 2;
      left: 15px;
      font-size: 20px;
      -webkit-box-shadow: -5px 10px 20px 0px rgba(0, 0, 0, 0.1);
      -moz-box-shadow: -5px 10px 20px 0px rgba(0, 0, 0, 0.1);
      box-shadow: -5px 10px 20px 0px rgba(0, 0, 0, 0.1);
      &.right-arrow-box {
        left: inherit;
        right: 15px;
        -webkit-box-shadow: 5px 10px 20px 0px rgba(0, 0, 0, 0.1);
        -moz-box-shadow: 5px 10px 20px 0px rgba(0, 0, 0, 0.1);
        box-shadow: 5px 10px 20px 0px rgba(0, 0, 0, 0.1);
      }
    }
    &:after {
      content: '';
      display: table;
      clear: both;
    }
    li {
      &:focus {
        outline: none;
      }
      width: 20%;
      float: left;
      font-size: 13px;
      .rdm_sld_img {
        max-width: 350px;
        margin: 0 auto;
        @media(min-width: 767px) {
          max-width: 230px;
        }
        img {
        width: 100%;
        }
      }
      h5 {
        @include font($grb,
        normal);
        margin-top: 50px;
      }
      .price {
        margin: 10px 0;
        @include font($grb,
        normal);
        font-weight: 400;
        color: #f25b29;
      }
    }
  }
  // redeem product end
  .faq-section {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 15px;
    a {
      color: #444;
      span {
        font-weight: bold;
        color: $scnd;
      }
    }
  }
  .logout-button-holders {
    max-width: 190px;
    margin: 0 auto;
    .logout_btn {
      width: 184px;
      height: 54px;
      background: $scnd;
      font-size: 13px;
      display: block;
      color: $white;
      text-decoration: none;
      @include display(flex,
      center,
      center);
      border-radius: 40px;
      margin: 20px 10px;
      border: 2px solid $scnd;
      &:hover {
        color: #ffffff;
        background: #2d2d2d;
        border-color: #2d2d2d;
      }
    }
  }
  // raf banner
  .raf_banner {
    img {
      width: 100%;
      object-fit: cover;
      object-position: top center;
      @media(min-width: 767px) {
        min-height: 274px;
      }
    }
  }
  .swell-referral-loader {
    text-align: center;
  }
  .sr_wrap {
    padding: 0 15px;
    @media (min-width: 767px) {
      display: flex;
      align-items: center;
      justify-content: center;
    }
    .swell-referral {
      max-width: 460px;
      width: 100%;
      margin: 0 auto 40px;
      text-align: center;
      color: #999999;
      @media (min-width: 767px) {
        margin: 0;
      }
      .refer-customer-error {
        color: red;
      }
      .swell-referral-sidebar-details-action {
        font-size: 11px;
        letter-spacing: 4px;
        text-transform: uppercase;
      }
      .swell-referral-heading {
        @extend %sec_title;
        color: #444444;
        margin: 15px 0 20px;
      }
      .swell-referral-details {
        font-size: 15px;
        max-width: 335px;
        margin: 0 auto;
      }
      .swell-referral-form-header-details {
        display: none;
      }
      .swell-referral-form-list-field {
        padding-right: 120px;
        position: relative;
        margin-top: 30px;
        .swell-referral-form-list-field-input {
          height: 47px;
          width: 100%;
          border-radius: 30px 0 0 30px;
          border: 1px solid #dadada;
          padding: 0 25px;
          &::-webkit-input-placeholder {
            color: #808080;
          }
          &::-moz-placeholder {
            color: #808080;
          }
          &:-ms-input-placeholder {
            color: #808080;
          }
          &:-moz-placeholder {
            color: #808080;
          }
        }
        .swell-referral-form-list-submit {
          position: absolute;
          height: 47px;
          width: 47px;
          border-radius: 0 30px 30px 0;
          background: $scnd;
          color: #fff;
          right: 0;
          top: 0;
