@import url('https://fonts.googleapis.com/css?family=Montserrat:400,700|Playfair+Display');
$imgpath: includes/images;
$default_clr: #444444;
$white: #fff;
$black: #000;
$scnd: #f25b29;
$pr: 'ProximaNova-Regular';
$grb: 'GothamRounded-Book';
$bt: 'Brandon Text';
@mixin display($display,
$justify: '',
$align: '') {
  @if $display=='flex' {
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    @if $justify !='' {
      -webkit-justify-content: $justify;
      -moz-justify-content: $justify;
      -ms-justify-content: $justify;
      -o-justify-content: $justify;
      justify-content: $justify;
    }
    @if $align !='' {
      -webkit-align-items: $align;
      -moz-align-items: $align;
      -ms-align-items: $align;
      -o-align-items: $align;
      align-items: $align;
    }
  }
  @else {
    display: block;
  }
}

@mixin font($ff:'',
$fw: '') {
  @if $ff !='' {
    font-family: $ff;
  }
  @if $fw !='' {
    font-weight: $fw;
  }
}

%sec_title {
  font-size: 26px;
  text-transform: uppercase;
  @include font($grb,
  normal);
}

@font-face {
    font-family: 'Gotham';
    src: url('Gotham-Bold.woff2') format('woff2'),
        url('Gotham-Bold.woff') format('woff');
    font-weight: bold;
    font-style: normal;
}


/* icons */
